{
  "name": "SlaHistory",
  "description": "Extension to keep Slack history",
  "version": "0.0.0.2",
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs", "http://*/*", "https://*/*",  "downloads", "storage"
  ],
  "web_accessible_resources": [
    "inject.html","historyPopup.html"
    ],
  "manifest_version": 2
}
